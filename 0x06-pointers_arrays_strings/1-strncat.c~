#include "holberton.h"

/**
 *_strncat - Write a function that concatenates two strings.
 *@dest: Destino 
 *@src: Pointer to source
 *@n: input bytes
 *Return: Success (dest)
 */

char *_strncat(char *dest, char *src, int n)
{
int count_dest; /* dest length*/
int src_pos; /*src position*/

for (count_dest = 0; dest[count_dest] != '\0'; count_dest++)
{

}
for (src_pos = 0; src_pos < n && src[src_pos] != '\0'; src_pos++)
{
dest[count_dest + src_pos] = src[src_pos];
}
dest[count_dest + src_pos] = '\0';
return (dest);
}
