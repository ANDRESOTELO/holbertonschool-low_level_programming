char *_strncat(char *dest, char *src, int n)
{
  int dest_length;
  int src_pos;

  char *_strncat(char *dest, char *src, int n);  for (dest_length = 0; dest[dest_length] != '\0'; dest_length++)
    {
    }
  for (src_pos = 0; src[src_pos] != '\0'; src_pos++)
    {
      dest[dest_length] = src[src_pos];
      
    }


}